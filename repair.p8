pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
rot=0

function _update()
 rot=flr(time()*2)%16
end

function _draw()
 rectfill(0,0,127,127,0)

 idx=rot%8
 if rot>=8 then
  --invert rear/front lights
  pal(8,10)
  pal(10,8)
 end
 spr(64+idx*2,60,60,2,2)
 pal()
end
