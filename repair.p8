pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
rot=0

function drawmap()
 for x=0,9 do
  for y=0,7 do
   m=mget(x,y)
   if m>0 then
    si=128+(m-16)*2
    if m>=24 then
     si+=16
    end
    spr(si,x*13,y*13,2,2)

    pset(x*13,y*13,0)
    pset(x*13+12,y*13,0)
    pset(x*13,y*13+12,0)
    pset(x*13+12,y*13+12,0)
   end
  end
 end
end

function _update()
 rot=flr(time()*2)%16
end

function _draw()
 rectfill(0,0,127,127,0)

 drawmap()

 idx=rot%8
 if rot>=8 then
  --invert rear/front lights
  pal(8,10)
  pal(10,8)
 end
 spr(64+idx*2,60,60,2,2)
 pal()
end
