 curs=gridcursor:new(
  vector:new(4,3)
 )

 _update=update_game
 _draw=draw_game

 --music(0)
end

function update_game()
 grid:update()
 foreach(bots,bot.update)
 curs:update()
 tray:update()
end

function draw_game()
 cls()

 grid:draw()
 foreach(bots,bot.draw)
 curs:draw()
 tray:draw()
end

function on_move(bot)
 printh("bot moved to "..bot.pos:to_string())
end

function on_crash(bot)
 printh("bot crashed")
end

